/* ntweb */
/*********/
body {
  display: grid;
  grid-template-rows: auto 1fr auto;
  grid-template-areas: "header" " main " "footer"; }

@media (min-width: 1024px) {
  body {
    grid-template-columns: auto 90ex auto;
    grid-template-areas: ". header ." ".  main  ." ". footer ."; } }

/* Header */
body > header {
  padding: 0em 1em;
  text-align: center;
  grid-area: header; }

body > header a {
  text-decoration: none; }

nav a.is-active {
  font-weight: bold;
  text-decoration: underline; }

.primary-menu {
  display: flex;
  flex-flow: row wrap;
  justify-content: center; }

.primary-menu .menu-item {
  margin-left: 1ex; }

.secondary-menu {
  display: none; }

.navbar-search {
  display: inline-block;
  position: relative; }

.navbar-search input {
  width: 15ex;
  border: none;
  border-bottom: 0.1em solid;
  margin-right: 1ex; }

@media (hover: hover) and (min-width: 768px) {
  .navbar-search input {
    width: 0ex;
    border: none;
    margin-right: 0em;
    transition: 0.25s; }
  .navbar-search:hover input,
  .navbar-search input:focus {
    width: 20ex;
    border-bottom: 0.1em solid;
    margin-right: 1ex;
    transition: 0.25s; } }

.navbar-search button {
  background: none;
  border: none;
  cursor: pointer;
  padding: 0; }

.navbar-search svg {
  color: #427819;
  color: var(--accent-color);
  position: relative;
  top: 0.2em; }

/* Main */
body > main {
  max-width: 100vw;
  box-sizing: border-box;
  padding: 0em 1em;
  padding-bottom: 1em;
  grid-area: main;
  display: flex;
  flex-flow: column; }

/* Aside */
.article-sidebar {
  margin-top: 1em;
  margin-left: 1em; }

.article-sidebar > div {
  display: flex;
  flex-flow: column;
  align-items: center; }

/* TOC */
.article-toc ul {
  list-style: initial; }

/* Related */
.article-related-content-title {
  text-align: center; }

.article-related-content-items {
  list-style: initial;
  padding: 0 1em; }

/* Sharing */
.article-sharing .icon {
  width: 1.5em;
  margin-top: 1em;
  margin-left: 0.5em; }

/* Series */
.article-series-serie th,
.article-series-serie td {
  background: var(--background);
  border: 0.1em solid;
  padding: 0.5em 1em; }

.article-series-serie th a {
  text-decoration: none; }

.article-series-serie td a {
  color: inherit; }

.article-series-serie td {
  font-size: 0.85em; }

.article-series-serie td.is-active {
  border: 0.2em solid;
  font-weight: bold; }

@media print {
  .article-sidebar {
    display: none; } }

@media screen and (min-width: 130ex) {
  .has-aside {
    grid-template-columns: auto 90ex auto;
    grid-template-areas: ".    header ." "main  main  main" ".    footer ."; }
  .has-aside > main {
    flex-flow: row-reverse;
    justify-content: center; }
  .article-sidebar {
    width: 40ex;
    position: relative; }
  .article-sidebar > div {
    width: 40ex;
    max-width: 40ex;
    max-height: 75vh;
    overflow: auto;
    position: fixed;
    top: 20vh; }
  .article {
    width: 90ex; } }

/* Article */
.article > header {
  margin-bottom: 1em; }

.article-image {
  width: 100%;
  max-height: 20em;
  object-fit: contain;
  object-position: center; }

.article-title {
  font-size: 2.5em; }

.article-description {
  font-size: 0.8em;
  margin-top: -1em; }

.article-metadata {
  font-size: 0.8em; }

.markdown h2::before,
.markdown h3::before,
.markdown h4::before,
.markdown h5::before,
.markdown h6::before {
  opacity: 0.25; }

.markdown h2::before {
  content: "# "; }

.markdown h3::before {
  content: "## "; }

.markdown h4::before {
  content: "### "; }

.markdown h5::before {
  content: "#### "; }

.markdown h6::before {
  content: "##### "; }

@media print, screen and (min-width: 768px) {
  .article.has-image > header {
    display: grid;
    grid-template-areas: "article-image  article-title" "article-image  article-description" "article-image  article-metadata"; }
  .article-image {
    width: 15em;
    grid-area: article-image;
    margin: auto; }
  .article-title {
    grid-area: article-title; }
  .article-description {
    grid-area: article-description; }
  .article-metadata {
    grid-area: article-metadata; } }

/* Pagination */
.pagination {
  text-align: center; }

/* Comments */
.article-comments {
  margin-top: 2em; }

.article-comments #comments-toggler {
  border: 0.25em dashed;
  display: block;
  font-size: 1.15em;
  text-align: center;
  padding: 0.5em 1em; }

.article-comments #comments-toggler a {
  text-decoration: none; }

/* Footer */
body > footer {
  grid-area: footer;
  border-top: 0.05em solid;
  font-size: 0.8em;
  padding: 1em 0em;
  text-align: center; }

/* Privacy alert */
.privacy-alert {
  background: #ccc;
  color: #000;
  width: 90vw;
  max-height: 15em;
  background: var(--font-color);
  border: 0.5em dashed;
  border-radius: 0.5em;
  box-sizing: border-box;
  color: var(--background);
  display: none;
  overflow: auto;
  padding: 1em 2em;
  position: fixed;
  left: 5vw;
  bottom: 1em; }

.privacy-alert::after {
  content: "X";
  cursor: pointer;
  opacity: 0.8;
  position: absolute;
  top: 1em;
  right: 1em; }

.privacy-alert-header {
  font-size: 1.25em;
  font-weight: bold; }

@media (min-width: 1024px) {
  .privacy-alert {
    width: 80ex;
    left: calc(50% - 40ex); } }

/* Printing */
@media print {
  body {
    background: transparent;
    color: #24292e;
    display: block;
    line-height: 1.5; }
  body > header,
  body > footer {
    display: none; }
  body > main {
    width: 100%; } }

/* Table of contents */
.toc {
  padding: 1em;
  padding-bottom: 0em; }

/* Notes */
.note {
  border: 0.1em solid;
  margin: 2em 0em;
  padding: 1em 2em; }

.note .note-header {
  border-bottom: 0.1em solid;
  font-weight: bold;
  margin-top: 0.5em;
  margin-bottom: 1em;
  padding-bottom: 0.5em; }

/* Details */
.details:not(.no-border) {
  border: 0.1em solid;
  margin: 2em 0em;
  padding: 1em 2em; }

/* Mermaid */
.mermaid-output svg {
  background: #fff; }

/* Cards */
.page-card {
  max-width: 100%;
  border: 0.1em dashed;
  box-sizing: border-box;
  margin: 0.5em;
  position: relative; }

.page-card-image {
  width: 100%;
  max-height: 20em;
  object-fit: contain;
  object-position: center; }

.page-card header {
  padding: 1em 1.5em; }

.page-card-title {
  font-size: 1.5em; }

.page-card-description {
  font-size: 0.85em;
  margin: 0em; }

.page-card-metadata {
  font-size: 0.85em;
  opacity: 0.8; }

@media print, screen and (min-width: 768px) {
  .page-card.has-image {
    display: flex; }
  .page-card.has-image .page-card-image-link {
    min-width: 15em;
    max-width: 15em;
    display: flex; }
  .page-card-image {
    float: left;
    position: relative; } }

/* Params */
.params {
  border: 0.1em solid; }

.param {
  border-bottom: 0.1em dashed;
  padding: 1em 2em; }

.param:last-child {
  border-bottom: none; }

.param-header {
  font-weight: bold; }

.param-body p:first-child {
  margin-top: 0em; }

:root {
  --font-family: monospace;
  --font-size: 16px; }

@media (min-width: 1400px) {
  :root {
    --font-size: 18px; } }

@media (min-width: 1900px) {
  :root {
    --font-size: 22px; } }

/* Elements */
body {
  background: #fff;
  color: #24292e;
  background: var(--background);
  color: var(--font-color);
  font-family: var(--font-family);
  font-size: var(--font-size);
  line-height: 2; }

a {
  color: #427819;
  color: var(--accent-color); }

input, select, textarea {
  color: #ccc;
  background: transparent;
  color: var(--font-color); }

p {
  -ms-hyphens: auto;
  -webkit-hyphens: auto;
  hyphens: auto; }

select {
  padding: 0.25em 0.5em; }

[disabled] {
  cursor: not-allowed; }

li ul,
li ol {
  padding-left: 1em; }

table {
  border-collapse: separate;
  border-spacing: 0.15em;
  margin: 1em auto; }

thead th {
  color: #427819;
  color: var(--primary-color); }

kbd {
  background: #eee;
  color: #333;
  background: var(--kbd-background);
  color: var(--kbd-color); }

pre {
  margin-top: 1em;
  margin-bottom: 1em; }

code,
pre,
pre > code,
pre code {
  background: #eee;
  color: #24292e;
  background: var(--code-background);
  color: var(--code-color);
  -ms-hyphens: none;
  -webkit-hyphens: none;
  hyphens: none; }

.highlight table {
  border-collapse: collapse;
  border-spacing: 0;
  margin: 0em; }

/* Classes */
.center {
  margin-left: auto;
  margin-right: auto;
  text-align: center; }

.hidden {
  width: 0em;
  overflow: hidden;
  visibility: hidden;
  position: fixed;
  top: 200vh;
  left: 200vw; }

.muted {
  opacity: 0.8; }

.small {
  font-size: 0.85em; }

.icon {
  width: 1em;
  display: inline-block;
  font-size: 1em;
  position: relative; }

.icon svg {
  max-width: 100%;
  max-height: 100%; }

img.center,
figure.center img {
  display: block;
  margin-left: auto;
  margin-right: auto; }

img.left,
figure.left {
  float: left;
  margin-right: 1em; }

img.right,
figure.right {
  float: right;
  margin-left: 1em; }

figure.center figcaption,
figure.left figcaption,
figure.right figcaption {
  text-align: center; }

/* Themes */
body,
[data-theme="light"] {
  --background: #fff;
  --primary-color: #427819;
  --accent-color: #427819;
  --font-color: #24292e;
  --code-background: #eee;
  --code-color: var(--font-color);
  --kbd-background: #eee;
  --kbd-color: #333; }

[data-theme="dark"] {
  --background: #212121;
  --primary-color: #427819;
  --accent-color: #427819;
  --font-color: #ccc;
  --code-background: #272822;
  --code-color: var(--font-color);
  --kbd-background: #eee;
  --kbd-color: #333; }

[data-theme="black"] {
  --background: #000;
  --primary-color: #427819;
  --accent-color: #427819;
  --font-color: #ccc;
  --code-background: #272822;
  --code-color: var(--font-color);
  --kbd-background: #eee;
  --kbd-color: #333; }

